    <script type="text/javascript">
        $(function () {
            $.ajax({
                url: '/Home/CheckSessionTimeOut',
                type: "POST",
                success: function (result) {
                    if (result == "True") {
                        window.top.location = "/Account/Login";
                    }
                },
                complete: function () {

                }
            });
            $('#txtFromDate').datepicker();
            $('#txtToDate').datepicker();
            $('#loading').hide();

            $('#tblDataGrid2').datagrid({
                title: 'Voucher Details',
                toolbar: '#tb',
                pagination: true,
                pageNumber: 1,
                pageSize: 10,
                multipleselect: true,
                showFooter: true,
                rownumbers: true,
                columns: [[
                            { field: 'Name', title: 'Name', width: 300, align: 'left' },
                            { field: 'Detail', title: 'Details', width: 300, align: 'left' },
                            { field: 'Amount', title: 'Amount', width: 300, align: 'left' }
                ]]
            });
            Close_Dialog();

        });

        function Clear() {
            $('#txtFromDate').val('');
            $('#txtToDate').val('');
        }
        function Search() {
            var reportType = $('#ddlOption').val();
            var frmDate = $('#txtFromDate').val();
            var toDate = $('#txtToDate').val();
            $.ajax({
                url: '/Approval/GetOptionList',
                type: "POST",
                dataType: 'json',
                data: JSON.stringify({ fromDate: frmDate, toDate: toDate, type: reportType }),
                async: true,
                contentType: 'application/json',
                success: function (data) {
                    $('#tblDataGrid').datagrid('loadData', data);
                },
                complete: function () {
                    $('#loading').hide();
                }
            });
        }
        function Approve() {
            var list = [];
            var strs = '';
            var selectedValues = $('#tblDataGrid').datagrid('getChecked');
            if (selectedValues.length <= 0) {
                toastr.warning("Please Select Atleast One Item");
                return;
            }
            $.grep(selectedValues, function (item, row) {
                var itm = { OptionNo: item.OptionNo,Amount:0,Details:"" };
                list.push(itm);
            });
            $('#loading').show();
            $.ajax({
                url: '/Approval/Approval',
                type: "POST",
                dataType: 'json',
                data: JSON.stringify({ list: list,type:'V' }),
                async: true,
                contentType: 'application/json',
                success: function (data) {
                    toastr.success(data.Message);
                },
                complete: function () {
                    $('#loading').hide();
                }
            });
        }
        function Open_Dialog() {
            $('#divVoucherDetails').dialog('open');
        }
        function Close_Dialog() {
            $('#divVoucherDetails').dialog('close');
        }
        function DetailView(voucherid, voucherno) {
            $.ajax({
                url: '/Approval/GetVoucherDetails',
                type: "POST",
                dataType: 'json',
                data: JSON.stringify({ voucherid:voucherid,voucherno: voucherno }),
                async: true,
                contentType: 'application/json',
                success: function (data) {
                    Open_Dialog();
                    $('#tblDataGrid2').datagrid('loadData', data);
                },
                complete: function () {
                    $('#loading').hide();
                }
            });
        }
    </script>

</head>
<body>
<div style="padding: 0px; width: 100%; overflow: visible">
    <br/>

    <div class="onerow" id="reportOption">

        <div class="col2">Option</div>
        <div style="width: 60%;">
            <select id="ddlOption" style="width: 50%;">
                <option value="0">-------Select-------</option>
                <option value="SF">Reseller Fee</option>
                <option value="V">Voucher</option>
            </select>
        </div>
    </div>
    <div class="onerow" id="datetimePanel">
        <div class="col2">From Date</div>
        <div class="col2">
            <input type="text" id="txtFromDate" style="text-align: right"/>
        </div>
        <div class="col2">To Date</div>
        <div class="col2">
            <input type="text" id="txtToDate"/>
        </div>
    </div>
    <div class="onerow" id="SearchBox">
        <div class="col2"></div>
        <div class="col2">
            <input id="btnSearch" type="button" class="br-buttons" value="Search" onclick="Search()"/>
        </div>
        <div class="col2"></div>
        <div class="col2">

        </div>
    </div>
    <div class="onerow" id="OptionListDiv" style="width: 50%; height: 300px; overflow: scroll; text-align: center; margin-left: 180px;">
        <table id="tblDataGrid" class="easyui-datagrid" toolbar="#toolbar" autorowheight="true"
               fitcolumns="true"
               multipleselect="true"
               rownumbers="true"
               pagination="true"
               pagesize="10">
            <thead>
            <tr>
                <th data-options="field:'Status',checkbox:true">
                </th>
                <th field="OptionId" width="50" sortable="true" hidden="true"></th>
                <th field="OptionNo" width="50" sortable="true">Number</th>
                <th field="Amount" width="50" sortable="true">Amount</th>
                <th field="Details" width="50" sortable="true">Details</th>
            </tr>
            </thead>
        </table>
    </div>
    <div class="onerow" style="text-align: center">
        <br/>
        <br/>
        <input id="btnSave" type="button" class="br-buttons" value="Approve" onclick="Approve()"/>
        <input id="btnClose" type="button" class="br-buttons" value="Clear" onclick="Clear()"/>
    </div>
</div>
    <div class="easyui-dialog" id="divVoucherDetails" class="easyui-dialog" style="width: 900px;
        height: 400px; padding: 10px" data-options="title:'Copy',buttons:'#bb2',modal:true">
        <table id="tblDataGrid2" class="easyui-datagrid" toolbar="#toolbar" autorowheight="true" width="700px"
               fitcolumns="true"
               multipleselect="true"
               rownumbers="true"
               pagination="true"
               pagesize="10">
            <thead>
                <tr>
                    <th field="Name" width="50" sortable="true" hidden="true"></th>
                    <th field="Detail" width="50" sortable="true">Number</th>
                    <th field="Amount" width="50" sortable="true">Amount</th>
                </tr>
            </thead>
        </table>
    </div>
    <div id='loading'><div id='loader'>Loading...</div></div>
</body>
</html>
