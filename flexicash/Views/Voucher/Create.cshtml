    <script type="text/javascript">

        $(function () {
            $('#loading').hide();
            $.ajax({
                url: '/Home/CheckSessionTimeOut',
                type: "POST",
                success: function (result) {
                    if (result == "True") {
                        window.top.location = "/Account/Login";
                    }
                },
                complete: function () {
                    // setupSessionTimeoutCheck();
                }
            });

            
            jQuery(".heading").click(function () {
                var $content = jQuery(this).next(".content");
                var $img = jQuery(this).find(".imgShowHideDiv");
                if ($content.is(':visible')) {
                    $img.removeAttr("src");
                    $img.attr("src", "../../Content/images/sort_desc.png");
                }
                else {
                    $img.removeAttr("src");
                    $img.attr("src", "../../Content/images/sort_asc.png");
                }
                jQuery(this).next(".content").slideToggle(500);
            });
        });

    </script>
    <style type="text/css">
        .contentDetails {
            padding-left: 2%;
